		write /sys/devices/system/cpu/cpu2/cpufreq/interactive/use_sched_load 0
		write /sys/devices/system/cpu/cpu2/cpufreq/interactive/boostpulse_duration 0
    
    #Enable Input Boost for LITTLE cluster @537MHz for 40ms
		write /sys/module/cpu_boost/parameters/input_boost_enabled 1
		write /sys/module/cpu_boost/parameters/input_boost_freq "0:537600 1:537600 2:0 3:0"
		write /sys/module/cpu_boost/parameters/boost_ms 0
		write /sys/module/cpu_boost/parameters/input_boost_ms 40
		#Disable TouchBoost
		write /sys/module/msm_performance/parameters/touchboost 0
		# Disabling BCL and Removing Perfd
		write /sys/devices/soc.0/qcom,bcl.60/mode "disable"
    
	# Tweaking HMP Scheduler for increased Big Cluster utilization
		write /proc/sys/kernel/sched_window_stats_policy 2
		write /proc/sys/kernel/sched_ravg_hist_size 5
